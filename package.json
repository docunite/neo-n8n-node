{
  "name": "n8n-nodes-neo",
  "version": "0.1.6",
  "description": "n8n node for docuniteÂ® NEO API (docunite.com/neo) integration - The End-to-end AI Document Intelligence Pipeline",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "neo",
    "docunite",
    "document-management",
    "document-classification",
    "ai-documents",
    "webhook",
    "trigger",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/docunite/neo-n8n-node",
  "author": {
    "name": "docunite GmbH",
    "email": "support@docunite.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docunite/neo-n8n-node.git"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NeoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Neo/Neo.node.js",
      "dist/nodes/Neo/NeoTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
