{
  "name": "n8n-nodes-neo",
  "version": "0.1.0",
  "description": "n8n node for Neo API (docunite.ai) integration - Document management, classification, and AI-powered document processing",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "neo",
    "docunite",
    "document-management",
    "document-classification",
    "ai-documents"
  ],
  "license": "MIT",
  "homepage": "https://github.com/docunite/n8n-nodes-neo",
  "author": {
    "name": "docunite GmbH",
    "email": "support@docunite.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docunite/n8n-nodes-neo.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NeoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Neo/Neo.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
